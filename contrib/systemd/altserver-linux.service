[Unit]
Description=AltServer-Linux Daemon
After=network-online.target usbmuxd.service avahi-daemon.service altserver-anisette.service
Wants=network-online.target usbmuxd.service avahi-daemon.service altserver-anisette.service

[Service]
Type=simple
Environment=ALTSERVER_ANISETTE_SERVER=http://127.0.0.1:6969
ExecStart=/usr/local/bin/AltServer -d -d
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
